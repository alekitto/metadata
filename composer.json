{
  "name": "kcs/metadata",
  "description": "Class/Attribute metadata management component",
  "keywords": ["metadata", "annotations", "xml", "yaml"],
  "license": "MIT",
  "type": "library",
  "require": {
    "php": ">= 5.4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "doctrine/cache": "~1.4",
    "doctrine/annotations": "~1.2",
    "symfony/event-dispatcher": "~2.0|~3.0",
    "mikey179/vfsStream": "^1.6",
    "symfony/finder": "~2.0|~3.0"
  },
  "suggest": {
    "symfony/event-dispatcher": "~2.0|~3.0",
    "doctrine/cache": "~1.4",
    "symfony/finder": "~2.0|~3.0"
  },
  "autoload": {
    "psr-4": {
      "Kcs\\Metadata\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kcs\\Metadata\\Tests\\": "tests/"
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  },
  "authors": [
    {
      "name": "Alessandro Chitolina",
      "email": "alekitto@gmail.com"
    }
  ]
}
